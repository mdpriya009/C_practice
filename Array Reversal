#include <stdio.h>
#include <stdlib.h>

int main()
{
    int num, arr[100], i;
    scanf("%d", &num);
    
    for(i = 0; i < num; i++) {
        scanf("%d", arr[i]);
    }
    
    
    /* Write the logic to reverse the array. */
    
    for(i=num-1;i>=0;i--)
    {
        printf("%d",arr[num-i]);
    }

    return 0;
}
